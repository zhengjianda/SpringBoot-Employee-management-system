<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luo.dao.UserMapper">

    <select id="queryUserByName" parameterType="String" resultType="com.luo.pojo.User">
        select * from webtest.user where username=#{username}
    </select>
</mapper>